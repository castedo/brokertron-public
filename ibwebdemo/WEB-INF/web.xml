<web-app version="2.4" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/j2ee" xsi:schemalocation="http:/java.sun.com/dtd/web-app_2_3.dtd">
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>ibwebdemo</servlet-name>
    <servlet-class>com.brokertron.IBServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ibwebdemo</servlet-name>
    <url-pattern>/index.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ibwebdemo</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
</web-app>
